<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <httpProtocol>
            <customHeaders>
                <add name="Content-Security-Policy" value="default-src 'self' fonts.googleapis.com; img-src 'self' data:; font-src 'self' fonts.gstatic.com; connect-src api.assistantapps.com" />
                <add name="X-Content-Security-Policy" value="default-src 'self' fonts.googleapis.com; img-src 'self' data:; font-src 'self' fonts.gstatic.com; connect-src api.assistantapps.com" />
                <add name="X-WebKit-CSP" value="default-src 'self' fonts.googleapis.com; img-src 'self' data:; font-src 'self' fonts.gstatic.com; connect-src api.assistantapps.com" />
            </customHeaders>
        </httpProtocol>
    </system.webServer>
</configuration>
