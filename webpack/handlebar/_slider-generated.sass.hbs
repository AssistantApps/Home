$appStartTitles: {{#each assistantApps}}{{#unless @last}}"{{{titleStart}}}",{{/unless}}{{/each}}
$appEndTitles: {{#each assistantApps}}{{#unless @last}}"{{{titleEnd}}}",{{/unless}}{{/each}}
$appImages: {{#each assistantApps}}{{#unless @last}}"{{{icon}}}",{{/unless}}{{/each}}
$defaultAppImage: "{{assistantApps.0.icon}}"

$appAnimationPerc: (100 / length($appStartTitles))
$appAnimationGapPerc: 5
$appAnimationSecPerApp: 2.5
$appAnimationDuration: ($appAnimationSecPerApp * length($appStartTitles))